{
  "manifest_version": 3,
  "name": "OSM.org Human-readable Wikidata",
  "description": "Add human-readable information related to wikidata QIDs inline in osm.org",
  "version": "1.0",
   "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [],
  "background": {
    "service_worker": "service_worker.js"
  },
  "host_permissions": [
    "https://*.wikidata.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.openstreetmap.org/*",
        "https://openstreetmap.org/*"
      ],
      "js": ["osm-wikidata.js"]
    }
  ]
}
